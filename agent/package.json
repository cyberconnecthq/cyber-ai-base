{
    "name": "@ai16z/agent",
    "version": "0.0.1",
    "main": "src/index.ts",
    "bin": "src/index.ts",
    "type": "module",
    "pkg": {
        "targets": [
            "latest-linux-x64",
            "latest-macos-x64"
        ],
        "assets": []
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
        "start": "tsc && node --loader ts-node/esm src/index.ts",
        "dev": "tsc && nodemon --watch src --watch ../core/dist --ext ts,json --exec 'node --loader ts-node/esm' src/index.ts",
        "start:service:artist": "pm2 start pnpm --name=\"artist\" --restart-delay=3000 --max-restarts=10 -- run start --character=artist",
        "start:service:platform": "pm2 start pnpm --name=\"platform\" --restart-delay=3000 --max-restarts=10 -- run start --character=platform",
        "start:service:chiblings": "pm2 start pnpm --name=\"chiblings\" --restart-delay=3000 --max-restarts=10 -- run start --character=chiblings",
        "start:all": "node --loader ts-node/esm src/index.ts --characters=\"../characters/eliza.json\",\"../characters/degenspartan.json\",\"../characters/ruby.json\",\"../characters/pmairca.json\"",
        "package": "pkg ."
    },
    "dependencies": {
        "@ai16z/adapter-postgres": "workspace:*",
        "@ai16z/adapter-sqlite": "workspace:*",
        "@ai16z/client-direct": "workspace:*",
        "@ai16z/client-farcaster": "workspace:*",
        "@cyberlab/ai-client-twitter": "workspace:*",
        "@ai16z/eliza": "workspace:*",
        "@ai16z/plugin-bootstrap": "workspace:*",
        "@ai16z/plugin-node": "workspace:*",
        "plugin-nft-generation": "workspace:*",
        "readline": "1.3.0",
        "vite": "^6.0.1",
        "ws": "8.18.0",
        "yargs": "17.7.2"
    },
    "devDependencies": {
        "ts-node": "10.9.2",
        "tsup": "8.3.5",
        "vite-plugin-wasm": "^3.3.0"
    }
}
